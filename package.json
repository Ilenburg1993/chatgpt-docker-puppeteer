{
    "name": "chatgpt-docker-puppeteer",
    "version": "1.0.0",
    "description": "ðŸ¤– Autonomous AI agent for browser automation with Puppeteer - Domain-driven task processing with adaptive validation and real-time monitoring",
    "private": false,
    "main": "index.js",
    "type": "commonjs",
    "directories": {
        "lib": "src",
        "test": "tests",
        "scripts": "scripts",
        "doc": "DOCUMENTAÃ‡ÃƒO"
    },
    "files": [
        "src/",
        "scripts/",
        "tests/",
        "public/",
        "index.js",
        "ecosystem.config.js",
        "config.json",
        "dynamic_rules.json",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ],
    "scripts": {
        "setup": "bash scripts/setup.sh",
        "doctor": "bash scripts/doctor.sh",
        "start": "node index.js",
        "dev": "nodemon index.js --ignore fila/ --ignore logs/ --ignore respostas/",
        "daemon:start": "pm2 start ecosystem.config.js",
        "daemon:stop": "pm2 stop agente-gpt dashboard-web",
        "daemon:restart": "pm2 restart agente-gpt dashboard-web",
        "daemon:reload": "pm2 reload agente-gpt dashboard-web",
        "daemon:monit": "pm2 monit",
        "daemon:logs": "pm2 logs --lines 50",
        "daemon:flush": "pm2 flush",
        "daemon:kill": "pm2 delete all",
        "daemon:status": "pm2 status",
        "queue:status": "node scripts/status_fila.js",
        "queue:add": "node scripts/gerador_tarefa.js",
        "queue:import": "node scripts/importar_prompts.js",
        "queue:graph": "node scripts/visualizar_fila.js > fila.dot",
        "queue:flow": "node scripts/flow_manager.js",
        "analyze:deps": "npx madge --circular --extensions js src/",
        "analyze:deps:graph": "npx madge --circular --extensions js --image deps-graph.svg src/",
        "analyze:graph": "node scripts/analyze-code-graph.js --stats",
        "analyze:graph:full": "node scripts/analyze-code-graph.js --stats --circular --orphans --nerv",
        "analyze:graph:export": "node scripts/analyze-code-graph.js --export-json --export-dot",
        "analyze:circular": "node scripts/analyze-code-graph.js --circular",
        "analyze:orphans": "node scripts/analyze-code-graph.js --orphans",
        "analyze:nerv": "node scripts/analyze-code-graph.js --nerv",
        "vscode:extensions": "bash scripts/install-extensions.sh",
        "vscode:check": "node scripts/check-vscode-extensions.js",
        "vscode:list": "code --list-extensions --show-versions",
        "vscode:update": "code --update-extensions",
        "clean": "node -e \"const fs=require('fs'); ['logs', 'tmp', 'RUNNING.lock'].forEach(p => fs.rmSync(p, {recursive:true, force:true})); console.log('[OK] Ambiente limpo.');\"",
        "clean:queue": "node -e \"const fs=require('fs'); const path=require('path'); const D='fila'; if(fs.existsSync(D)) fs.readdirSync(D).filter(f=>f.endsWith('.json')).forEach(f=>fs.rmSync(path.join(D,f))); console.log('[OK] Fila limpa.');\"",
        "clean:logs": "rm -rf logs/*.log logs/crash_reports/*",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "lint:quiet": "eslint . --quiet",
        "lint:report": "eslint . --output-file logs/eslint-report.txt --format stylish",
        "lint:src": "eslint src/",
        "lint:tests": "eslint tests/",
        "format": "prettier --write \"**/*.{js,json,md}\"",
        "format:check": "prettier --check \"**/*.{js,json,md}\"",
        "format:src": "prettier --write \"src/**/*.js\"",
        "format:tests": "prettier --write \"tests/**/*.js\"",
        "maintenance": "node scripts/puppeteer_maintenance.js",
        "maintenance:clean-cache": "node scripts/puppeteer_maintenance.js --clean-cache",
        "profiles:rotate": "node scripts/rotate-profiles.js",
        "profiles:stats": "node -e \"require('./scripts/rotate-profiles').getBackupStats().then(s => console.log(JSON.stringify(s, null, 2)))\"",
        "diagnose": "node scripts/analisar_crash.js",
        "reset:hard": "npm run clean && npm run clean:queue && npm run clean:logs",
        "test": "node --test tests/**/*.spec.js",
        "test:all": "node scripts/run-all-tests.js",
        "test:report": "node scripts/run-all-tests.js",
        "test:unit": "node --test 'tests/unit/**/*.spec.js'",
        "test:integration": "node --test 'tests/integration/**/*.spec.js'",
        "test:e2e": "node --test 'tests/e2e/**/*.spec.js'",
        "test:regression": "node --test 'tests/regression/**/*.spec.js'",
        "test:coverage": "npx c8 npm test",
        "test:coverage:unit": "npx c8 npm run test:unit",
        "test:coverage:integration": "npx c8 npm run test:integration",
        "test:watch": "node --test --watch 'tests/**/*.spec.js'",
        "test:watch:unit": "node --test --watch 'tests/unit/**/*.spec.js'",
        "test:ci": "npx c8 --check-coverage npm test",
        "test:debug": "node --inspect-brk --test 'tests/**/*.spec.js'",
        "test:clean": "rm -rf coverage/ tests/tmp/ .c8/",
        "test:legacy": "node scripts/run-tests.js",
        "test:health": "node tests/test_health_endpoint.js",
        "test:config": "node tests/test_config_validation.js",
        "test:win": "powershell -ExecutionPolicy Bypass -File scripts/run_all_tests.ps1",
        "test:linux": "bash scripts/run_all_tests.sh",
        "test:lock": "node tests/test_lock.js",
        "test:stall": "node tests/test_stall_mitigation.js",
        "test:schema": "node scripts/test_schema_validation.js",
        "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') !== -1) throw new Error('Use npm instead of yarn')\"",
        "postinstall": "echo 'âœ… Dependencies installed successfully'",
        "prepare": "node -e \"try{require('husky')}catch(e){}\"",
        "validate": "node scripts/validate_config.js",
        "validate:pre-start": "npm run validate && echo Ready to start",
        "validate:all": "npm run lint:quiet && npm run format:check && npm test",
        "validate:code": "npm run lint:quiet && npm run format:check",
        "check": "npm run validate:code"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ilenburg1993/chatgpt-docker-puppeteer.git"
    },
    "bugs": {
        "url": "https://github.com/Ilenburg1993/chatgpt-docker-puppeteer/issues"

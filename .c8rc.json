{
  "all": true,
  "include": [
    "src/**/*.js",
    "index.js"
  ],
  "exclude": [
    "node_modules/**",
    "tests/**",
    "scripts/**",
    "tools/**",
    "analysis/**",
    "backups/**",
    "coverage/**",
    "public/**",
    "profile/**",
    "local-login/**",
    "*.config.js",
    "*.config.mjs",
    "**/*.spec.js",
    "**/*.test.js"
  ],
  "reporter": [
    "html",
    "text",
    "lcov",
    "json-summary"
  ],
  "report-dir": "coverage",
  "temp-directory": ".c8",
  "clean": true,
  "check-coverage": true,
  "lines": 80,
  "functions": 75,
  "branches": 75,
  "statements": 80,
  "per-file": false,
  "skip-full": false,
  "watermarks": {
    "lines": [
      70,
      80
    ],
    "functions": [
      65,
      75
    ],
    "branches": [
      65,
      75
    ],
    "statements": [
      70,
      80
    ]
  }
}

{
    "typescript.tsdk": "node_modules/typescript/lib",
    "dev.containers.gpuAvailability": "none",
    "git.enableSmartCommit": true,
    "git.ignoreLimitWarning": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.fetchOnPull": true,
    "git.pruneOnFetch": true,
    "git.followTagsWhenSync": true,
    "git.showProgress": true,
    "git.untrackedChanges": "separate",
    "git.mergeEditor": true,
    "git.branchProtection": [
        "main",
        "master"
    ],
    "cSpell.enabled": false,
    "cSpell.language": "en,pt-BR",
    "cSpell.enabledLanguageIds": [
        "markdown",
        "plaintext",
        "jsonc",
        "javascript",
        "javascriptreact",
        "json",
        "typescript"
    ],
    "cSpell.enableFiletypes": [
        "markdown",
        "plaintext",
        "jsonc",
        "javascript",
        "javascriptreact"
    ],
    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.options": {
        "overrideConfigFile": "eslint.config.mjs"
    },
    "eslint.run": "onType",
    "eslint.format.enable": true,
    "eslint.lintTask.enable": true,
    "eslint.alwaysShowStatus": true,
    "eslint.debug": false,
    "eslint.codeActionsOnSave.mode": "all",
    "eslint.problems.shortenToSingleLine": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "never"
    },
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.rulers": [
        100,
        120
    ],
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 120,
    "editor.wrappingIndent": "indent",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.indentation": true,
    "editor.guides.highlightActiveIndentation": true,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 120,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.maxLineCount": 5,
    "editor.linkedEditing": true,
    "editor.matchBrackets": "always",
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.smoothScrolling": true,
    "editor.mouseWheelZoom": true,
    "editor.fontLigatures": true,
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.occurrencesHighlight": "multiFile",
    "editor.selectionHighlight": true,
    "editor.hover.enabled": "on",
    "editor.hover.delay": 300,
    "editor.hover.sticky": true,
    "files.eol": "\n",
    "files.encoding": "utf8",
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.associations": {
        "*.json": "jsonc",
        "ecosystem.config.js": "javascript",
        "Dockerfile*": "dockerfile",
        "*.sh": "shellscript",
        "*.bat": "bat",
        "*.ps1": "powershell",
        "Makefile": "makefile",
        ".env*": "properties",
        "*.md": "markdown",
        "*.code-snippets": "jsonc"
    },
    "files.watcherExclude": {
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/logs/**": true,
        "**/tmp/**": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/fila/**": true,
        "**/respostas/**": true,
        "**/profile/**": true,
        "**/backups/**": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.vscode-test/**": true,
        "**/coverage/**": true
    },
    "search.exclude": {
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/logs/**": true,
        "**/coverage/**": true,
        "**/.tmp.*/**": true,
        "**/package-lock.json": true,
        "**/fila/**": true,
        "**/respostas/**": true,
        "**/profile/**": true,
        "**/backups/**": true,
        "**/*.min.js": true,
        "**/*.map": true,
        "**/.git/**": true
    },
    "javascript.validate.enable": false,
    "javascript.format.enable": false,
    "javascript.suggest.completeFunctionCalls": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.preferences.quoteStyle": "single",
    "javascript.preferences.importModuleSpecifier": "shortest",
    "js/ts.implicitProjectConfig.checkJs": false,
    "js/ts.implicitProjectConfig.strictNullChecks": false,
    "js/ts.implicitProjectConfig.strictFunctionTypes": false,
    "json.format.enable": true,
    "json.validate.enable": true,
    "typescript.validate.enable": false,
    "typescript.tsserver.useSeparateSyntaxServer": false,
    "typescript.disableAutomaticTypeAcquisition": false,
    "typescript.preferences.importModuleSpecifier": "relative",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.wordWrap": "on"
    },
    "github.copilot.enable": {
        "*": true,
        "javascript": true,
        "javascriptreact": true,
        "typescript": true,
        "json": true,
        "jsonc": true,
        "markdown": true,
        "yaml": true,
        "dockerfile": true,
        "shellscript": true,
        "makefile": true,
        "plaintext": false
    },
    "github.copilot.inlineSuggest.enable": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.advanced": {
        "length": 1000,
        "temperature": 0.2,
        "top_p": 1,
        "stops": {
            "*": [
                "\n\n\n"
            ]
        },
        "indentationMode": {
            "javascript": "spaces",
            "typescript": "spaces",
            "json": "spaces"
        }
    },
    "github.copilot.chat.localeOverride": "pt-br",
    "github.copilot.chat.welcomeMessage": "always",
    "editor.inlineSuggest.enabled": true,
    "editor.inlineSuggest.suppressSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": "on"
    },
    "editor.quickSuggestionsDelay": 0,
    "editor.parameterHints.enabled": true,
    "editor.suggest.preview": true,
    "editor.suggest.showInlineDetails": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.snippetSuggestions": "top",
    "editor.tabCompletion": "onlySnippets",
    "editor.suggest.showSnippets": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "javascript.suggest.autoImports": true,
    "javascript.suggest.enabled": true,
    "javascript.referencesCodeLens.enabled": true,
    "javascript.referencesCodeLens.showOnAllFunctions": false,
    "javascript.inlayHints.parameterNames.enabled": "literals",
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.inlayHints.variableTypes.enabled": false,
    "typescript.inlayHints.parameterNames.enabled": "literals",
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.variableTypes.enabled": false,
    "javascript.suggest.names": true,
    "javascript.suggest.paths": true,
    "javascript.suggest.completeJSDocs": true,
    "npm.enableScriptExplorer": true,
    "npm.scriptExplorerAction": "run",
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.tabs.enabled": true,
    "terminal.integrated.tabs.hideCondition": "never",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.persistentSessionReviveProcess": "onExitAndWindowClose",
    "terminal.integrated.enablePersistentSessions": true,
    "terminal.integrated.shellIntegration.enabled": true,
    "terminal.integrated.shellIntegration.decorationsEnabled": "both",
    "terminal.integrated.shellIntegration.history": 1000,
    "terminal.integrated.suggest.enabled": true,
    "terminal.integrated.suggest.quickSuggestions": true,
    "makefile.configureOnOpen": true,
    "makefile.extensionOutputFolder": "./.vscode",
    "makefile.launchConfigurations": [
        {
            "cwd": "/workspaces/chatgpt-docker-puppeteer",
            "binaryPath": "/workspaces/chatgpt-docker-puppeteer/Makefile",
            "binaryArgs": []
        }
    ],
    "debug.console.fontSize": 13,
    "debug.console.lineHeight": 18,
    "debug.console.wordWrap": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.sortOrder": "type",
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "package.json": "package-lock.json, .npmrc, .nvmrc, .node-version",
        "*.js": "${capture}.test.js, ${capture}.spec.js, ${capture}.map",
        "*.ts": "${capture}.test.ts, ${capture}.spec.ts, ${capture}.js, ${capture}.map",
        "Dockerfile": "Dockerfile.*, docker-compose.*.yml, .dockerignore",
        ".gitignore": ".gitattributes, .gitmodules, .gitkeep",
        "README.md": "CHANGELOG.md, CONTRIBUTING.md, LICENSE, SECURITY*.md",
        "ecosystem.config.js": "temp_pm2.json",
        "*.sh": "${capture}.bat, ${capture}.ps1",
        "tasks.json": "tasks.json.old, keybindings.json, *.code-snippets"
    },
    "workbench.colorTheme": "Default Dark+",
    "workbench.iconTheme": "vs-seti",
    "workbench.startupEditor": "none",
    "workbench.editor.enablePreview": false,
    "workbench.editor.closeEmptyGroups": true,
    "workbench.editor.highlightModifiedTabs": true,
    "problems.autoReveal": true,
    "problems.decorations.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "diffEditor.renderMarginRevertIcon": true,
    "diffEditor.diffAlgorithm": "advanced",
    "diffEditor.wordWrap": "on",
    "diffEditor.maxComputationTime": 5000,
    "diffEditor.renderIndicators": true,
    "scm.diffDecorations": "all",
    "scm.diffDecorationsGutterWidth": 3,
    "scm.alwaysShowProviders": true,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "security.workspace.trust.enabled": false,
    "breadcrumbs.enabled": true,
    "breadcrumbs.filePath": "on",
    "breadcrumbs.symbolPath": "on",
    "outline.showFiles": true,
    "outline.showModules": true,
    "outline.showFunctions": true,
    "outline.showVariables": true,
    "telemetry.telemetryLevel": "off",
    "redhat.telemetry.enabled": false,
    "update.showReleaseNotes": false,
    "extensions.autoCheckUpdates": false,
    "editor.accessibilitySupport": "off",
    "window.restoreWindows": "preserve",
    "window.newWindowDimensions": "inherit",
    "window.autoDetectColorScheme": false,
    "window.titleBarStyle": "custom",
    "editor.unicodeHighlight.ambiguousCharacters": true,
    "editor.unicodeHighlight.invisibleCharacters": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.selectionMode": "always",
    "editor.tabCompletion": "on",
    "editor.wordBasedSuggestions": "matchingDocuments",
    "editor.wordBasedSuggestionsMode": "matchingDocuments",
    "files.hotExit": "onExitAndWindowClose",
    "files.restoreUndoStack": true,
    "files.refactoring.autoSave": true,
    "search.smartCase": true,
    "search.globalFindClipboard": true,
    "search.quickOpen.includeHistory": true,
    "search.quickOpen.includeSymbols": true,
    "workbench.list.smoothScrolling": true,
    "workbench.tree.indent": 12,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 10,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.settings.enableNaturalLanguageSearch": false
}
